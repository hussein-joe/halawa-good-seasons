<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="BIGINT" hibernate-type="Long"></sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="date"></sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="Boolean"
			length="1">
		</sql-type>
		<sql-type jdbc-type="FLOAT" hibernate-type="double"
			not-null="true" precision="53">
		</sql-type>
		<sql-type jdbc-type="FLOAT" hibernate-type="Double"
			precision="53" not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="Integer"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="yes_no" length="1"
			not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-name="Needy_Case"
		match-catalog="Yassin_GoodSeasons" match-schema="dbo">
	</table-filter>
	<table-filter match-name="NeedyCase_Agent"
		match-catalog="Yassin_GoodSeasons" match-schema="dbo">
	</table-filter>
</hibernate-reverse-engineering>